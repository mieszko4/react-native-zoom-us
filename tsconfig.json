{
  "compilerOptions": {
    "noEmit": true,
    "module": "CommonJS",
    "target": "ES2019",
    "moduleResolution": "node",
    "lib": ["esnext", "es2018"],
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strictNullChecks": true,
    "noEmitOnError": false,
    "declaration": true
  }
}
